{
    "name": "mkhab7/laravel-magical-artisan",
    "description": "A package for easier use of laravel artisan",
    "license": "MIT",
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1"
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.19|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mkhab7\\MagicalArtisan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mkhab7\\MagicalArtisan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Mkhab7\\MagicalArtisan\\MagicalArtisanServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "mkhab7",
            "email": "myappgo123@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
